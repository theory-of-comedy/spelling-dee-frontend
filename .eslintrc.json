{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "react-app/jest"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "warn",
    "react/jsx-filename-extension": "warn",
    "react/jsx-one-expression-per-line": "off"
  }
}